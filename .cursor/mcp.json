{
  "mcpServers": {
    "my-awesome-api": {
      "command": "python",
      "args": [
        "run_mcp_server.py"
      ],
      "env": {
        "PYTHONPATH": "."
      }
    },
    "my-awesome-api-sse": {
      "command": "python",
      "args": [
        "-m",
        "src.mcp.server"
      ],
      "env": {
        "PYTHONPATH": ".",
        "MCP_SERVER_PORT": "8001"
      }
    }
  }
}